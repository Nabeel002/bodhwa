        {%- assign form_success = form.posted_successfully? -%}{%- assign form_errors = form.errors -%}{%- render "form-general-script", form_success: form_success, form_errors: form_errors -%}<script>if (Shopify) {

var hideElementId = 'address_province_{{form.id}}';
