    hideElementId = "address_province_container_{{form.id}}";
}
new Shopify.CountryProvinceSelector(
    'address_country_{{form.id}}',
    'address_province_{{form.id}}',
    {
        hideElement: hideElementId
    }
);

document.querySelectorAll('#address_form_{{form.id}} [button="delete"]').forEach( button => {
                            
    button.addEventListener('click', function(evt) {
     var target = button.closest('form').action;
     var confirmMessage = evt.target.dataset.confirmMessage;
 
     console.log(target);
 
     evt.preventDefault();
     evt.stopPropagation();
     // eslint-disable-next-line no-alert
     if (
       confirm(
         confirmMessage || 'Are you sure you wish to delete this address?'
       )
     ) {
       Shopify.postLink(target, {
         parameters: { _method: 'delete' }
       });
     }
   });
                         
});

}</script>{%- endform -%}
