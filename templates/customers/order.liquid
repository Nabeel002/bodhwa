{%- assign headerSnippetPath = "header-index" -%}{%- assign wfPage = "616a6acafdb3cbf120b1a61f" -%}{%- assign bodyClasses = "" -%}
  {%- section 'announcement-ribbon' -%}
  {%- section 'new-nav-pc' -%}
  <div data-node-type="commerce-order-confirmation-wrapper" data-wf-order-query="" data-wf-page-link-href-prefix="" class="w-commerce-commerceorderconfirmationcontainer order-confirmation">
    <div class="w-commerce-commercelayoutcontainer w-container">
      <div class="w-commerce-commercelayoutmain">
        <div class="w-commerce-commercecheckoutcustomerinfosummarywrapper">
          <div class="w-commerce-commercecheckoutsummaryblockheader">
            <h4 class="heading-3">Customer Information</h4>
          </div>
          <fieldset class="w-commerce-commercecheckoutblockcontent">
            <div class="w-commerce-commercecheckoutrow">
              <div class="w-commerce-commercecheckoutcolumn">
                <div class="w-commerce-commercecheckoutsummaryitem"><label class="w-commerce-commercecheckoutsummarylabel heading-3">Email</label>
                  <div class="heading-4">{{ order.email }}</div>
                </div>
              </div>
              {%- assign condition_value = order.shipping_methods.size | strip -%}{%- if condition_value == "true" -%}<div class="w-commerce-commercecheckoutcolumn">
                <div class="w-commerce-commercecheckoutsummaryitem"><label class="w-commerce-commercecheckoutsummarylabel heading-3">Shipping Address</label>
                  <div class="heading-4">{{ order.shipping_address.name }}</div>
                  <div class="heading-4">{{ order.shipping_address.address1 }}</div>
                  <div class="heading-4">{{ order.shipping_address.address2 }}</div>
                  <div class="w-commerce-commercecheckoutsummaryflexboxdiv">
                    <div class="w-commerce-commercecheckoutsummarytextspacingondiv heading-4">{{ order.shipping_address.city }}</div>
                    <div class="w-commerce-commercecheckoutsummarytextspacingondiv heading-4">{{ order.shipping_address.state }}</div>
                    <div class="w-commerce-commercecheckoutsummarytextspacingondiv heading-4">{{ order.shipping_address.zip }}</div>
                  </div>
                  <div class="heading-4">{{ order.shipping_address.country }}</div>
                </div>
              </div>{%- endif -%}
            </div>
          </fieldset>
        </div>
        {%- assign condition_value = order.shipping_methods.size | strip -%}{%- if condition_value == "true" -%}<div class="w-commerce-commercecheckoutshippingsummarywrapper">
          <div class="w-commerce-commercecheckoutsummaryblockheader">
            <h4 class="heading-3">Shipping Method</h4>
          </div>
          <fieldset class="w-commerce-commercecheckoutblockcontent block-content">
            <div class="w-commerce-commercecheckoutrow">
              <div class="w-commerce-commercecheckoutcolumn">
                <div class="w-commerce-commercecheckoutsummaryitem">
                  <div class="heading-4">{{ order.shipping_methods[0].title }}</div>
                  
                </div>
              </div>
            </div>
          </fieldset>
        </div>{%- endif -%}
        <div class="w-commerce-commercecheckoutpaymentsummarywrapper">
          <div class="w-commerce-commercecheckoutsummaryblockheader">
            <h4 class="heading-3">Payment Info</h4>
          </div>
        </div>
        <fieldset class="w-commerce-commercecheckoutblockcontent">
          <div class="w-commerce-commercecheckoutrow">
            <div class="w-commerce-commercecheckoutcolumn">
              <div class="w-commerce-commercecheckoutsummaryitem"><label class="w-commerce-commercecheckoutsummarylabel">Payment Info</label>
                <div class="w-commerce-commercecheckoutsummaryflexboxdiv">
                  <div class="w-commerce-commercecheckoutsummarytextspacingondiv heading-4">{{ order.transactions[0].payment_details.credit_card_company }}</div>
                  <div class="w-commerce-commercecheckoutsummarytextspacingondiv heading-4">{{ order.transactions[0].payment_details.credit_card_number }}</div>
                </div>
                <div class="w-commerce-commercecheckoutsummaryflexboxdiv">
                  
                  {%- if order. -%}<div class="heading-4"> / </div>{%- endif -%}
                  
                </div>
              </div>
            </div>
            <div class="w-commerce-commercecheckoutcolumn">
              <div class="w-commerce-commercecheckoutsummaryitem"><label class="w-commerce-commercecheckoutsummarylabel heading-4">Billing Address</label>
                <div class="heading-4">{{ order.billing_address.name }}</div>
                <div class="heading-4">{{ order.billing_address.address1 }}</div>
                <div class="heading-4">{{ order.billing_address.address2 }}</div>
                <div class="w-commerce-commercecheckoutsummaryflexboxdiv">
                  <div class="w-commerce-commercecheckoutsummarytextspacingondiv heading-4">{{ order.billing_address.city }}</div>
                  <div class="w-commerce-commercecheckoutsummarytextspacingondiv heading-4">{{ order.billing_address.state }}</div>
                  <div class="w-commerce-commercecheckoutsummarytextspacingondiv heading-4">{{ order.billing_address.zip }}</div>
                </div>
                <div class="heading-4">{{ order.billing_address.country }}</div>
              </div>
            </div>
          </div>
        </fieldset>
        <div class="w-commerce-commercecheckoutorderitemswrapper">
          <div class="w-commerce-commercecheckoutsummaryblockheader block-header">
            <h4 class="heading-6">Items in Order</h4>
          </div>
          <fieldset class="w-commerce-commercecheckoutblockcontent">
            <script type="text/x-wf-template" id="wf-template-6155460ad9695a160a923a8e000000000051"><div data-node-type="checkout-order-item" id="" class="w-commerce-commercecheckoutorderitem"><img data-node-type="cart-item-image" template-part-bind="6155460ad9695a160a923a8e000000000053" class="w-commerce-commercecartitemimage" src="<%= it.image %>"><div data-node-type="checkout-order-item-description-wrapper" id="" class="w-commerce-commercecheckoutorderitemdescriptionwrapper"><div data-node-type="cart-product-name" class="heading-4 w-commerce-commercecartproductname" template-part-bind="6155460ad9695a160a923a8e000000000055"><%= it.title %></div><div data-node-type="checkout-order-item-quantity-wrapper" id="" class="w-commerce-commercecheckoutorderitemquantitywrapper"><div class="text-block-11" id="">Quantity: </div><div id="" template-part-bind="6155460ad9695a160a923a8e000000000059"><%= it.count %></div></div><ul data-node-type="checkout-order-item-option-list" id="" class="w-commerce-commercecheckoutoptionlist"><% Object.entries(it.properties).forEach( data =>  { %><li data-node-type="checkout-order-item-option-list-item" class="option-2" id=""><span data-node-type="checkout-order-item-option-list-item-label"><%= data[0] %></span><span id="">: </span><span data-node-type="checkout-order-item-option-list-item-value"><%= data[1] %></span></li><% }) %></ul></div><div class="heading-4" id="" template-part-bind="6155460ad9695a160a923a8e000000000060"><%= it.rowTotal %></div></div></script>
            <div role="list" class="w-commerce-commercecheckoutorderitemslist" data-wf-collection="database.commerceOrder.userItems" data-wf-template-id="wf-template-6155460ad9695a160a923a8e000000000051">{%- render 'order-items' -%}</div>
          </fieldset>
        </div>
      </div>
      <div class="w-commerce-commercelayoutsidebar">
        <div class="w-commerce-commercecheckoutordersummarywrapper">
          <div class="w-commerce-commercecheckoutsummaryblockheader">
            <h4 class="heading-3">Order Summary</h4>
          </div>
          <fieldset class="w-commerce-commercecheckoutblockcontent">
            <div class="w-commerce-commercecheckoutsummarylineitem">
              <div class="text-block-12">Subtotal</div>
              <div class="text-block-13">{{ order.subtotal_price | money }}</div>
            </div>
            <script type="text/x-wf-template" id="wf-template-6155460ad9695a160a923a8e00000000006b">%3Cdiv%20class%3D%22w-commerce-commercecheckoutordersummaryextraitemslistitem%22%3E%3Cdiv%20bind%3D%226155460ad9695a160a923a8e00000000006d%22%20class%3D%22text-block-14%22%3E%3C%2Fdiv%3E%3Cdiv%20bind%3D%226155460ad9695a160a923a8e00000000006e%22%20class%3D%22text-block-15%22%3E%3C%2Fdiv%3E%3C%2Fdiv%3E</script>
            <div class="w-commerce-commercecheckoutordersummaryextraitemslist" data-wf-collection="database.commerceOrder.extraItems" data-wf-template-id="wf-template-6155460ad9695a160a923a8e00000000006b">{%- for tax_line in order.tax_lines -%}<div class="w-commerce-commercecheckoutordersummaryextraitemslistitem"><div class="text-block-14">{{ tax_line.title }} ({{ tax_line.rate | times: 100 }}%): </div><div class="text-block-15">{{ tax_line.price | money }}</div></div>{%- endfor -%}</div>
            <div class="w-commerce-commercecheckoutsummarylineitem">
              <div class="text-block-16">Total</div>
              <div class="w-commerce-commercecheckoutsummarytotal heading-4-caps">{{ order.total_price | money }}</div>
            </div>
          </fieldset>
        </div>
      </div>
    </div>
  </div>
  {%- render 'footer' -%}
  <script src="https://d3e54v103j8qbb.cloudfront.net/js/jquery-3.5.1.min.dc5e7f18c8.js?site=613b36b97ccfb87f99eaeb93" type="text/javascript" integrity="sha256-9/aliU8dGd2tb6OSsuzixeV4y/faTqgFtohetphbbj0=" crossorigin="anonymous"></script>
  <script src="{{ 'webflow.js' | asset_url }}" type="text/javascript"></script>
  <!-- [if lte IE 9]><script src="https://cdnjs.cloudflare.com/ajax/libs/placeholders/3.0.2/placeholders.min.js"></script><![endif] -->

